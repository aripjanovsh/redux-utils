{
  "private": false,
  "name": "@dc0de/redux-utils",
  "sideEffects": false,
  "version": "0.0.11",
  "description": "Common utils for redux ecosystem",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "typings": "typings/index.d.ts",
  "repository": "https://github.com/dcodeteam/redux-utils.git",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/config/jest.js"
    ],
    "transform": {
      ".ts": "ts-jest"
    },
    "testMatch": [
      "**/src/**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/__tests__/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "scripts": {
    "ci": "yarn lint && yarn dts && yarn cover --ci",
    "all": "yarn lint --fix && yarn dts && yarn cover && yarn build",
    "test": "jest",
    "cover": "yarn test --coverage",
    "codecov": "codecov -f ./coverage/lcov.info",
    "report": "opener ./coverage/lcov-report/index.html",
    "dts": "tsc --noEmit",
    "lint": "js-scripts lint",
    "build": "yarn build:es && yarn build:cjs",
    "build:es": "rollup -c ./config/rollup.es.js",
    "build:cjs": "rollup -c ./config/rollup.cjs.js"
  },
  "dependencies": {
    "tslib": "^1.9.0"
  },
  "peerDependencies": {
    "immupdate": "^1.2.1",
    "redux": "^4.0.0",
    "rxjs": "^6.0.0"
  },
  "optionalDependencies": {
    "redux-persist": "^5.10.0"
  },
  "devDependencies": {
    "@dc0de/eslint-config": "^0.1.2",
    "@dc0de/eslint-config-node": "^0.1.2",
    "@dc0de/js-scripts": "^0.1.1",
    "@types/jest": "^23.1.2",
    "@types/node": "^10.3.6",
    "@types/react": "^16.4.7",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "immupdate": "^1.2.1",
    "jest": "^23.2.0",
    "opener": "^1.4.3",
    "redux": "^4.0.0",
    "rollup": "^0.63.4",
    "rollup-plugin-cleaner": "^0.2.0",
    "rollup-plugin-prettier": "^0.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "rxjs": "^6.2.1",
    "source-map-support": "^0.5.6",
    "ts-jest": "^23.1.2",
    "typescript": "^3.0.1"
  }
}
